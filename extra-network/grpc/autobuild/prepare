abinfo "Linking google test source folder..."
ln -sfv "$SRCDIR"/../googletest-release-$__GOOGLETEST_VER/{googlemock,googletest} \
	"$SRCDIR"/third_party/googletest

abinfo "Linking abseil-cpp..."
rm -rv "$SRCDIR"/third_party/abseil-cpp
ln -sfv "$SRCDIR"/../abseil-cpp-$__ABSEIL_VER \
	"$SRCDIR"/third_party/abseil-cpp

#unset CPPFLAGS CFLAGS CXXFLAGS LDFLAGS

export LD_LIBRARY_PATH="${SRCDIR}/abbuild"
